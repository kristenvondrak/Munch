<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HoloCircleSeekBar">
    <attr name="wheel_size" format="integer" />
    <attr name="pointer_size" format="dimension" />
    <attr name="max" format="integer"/>
    <attr name="show_text" format="boolean"/>
    <attr name="start_angle" format="integer"/>
    <attr name="end_angle" format="integer"/>
    <attr name="text_size" format="dimension"/>
    <attr name="init_position" format="integer"/>
    <attr name="wheel_active_color" format="string"/>
    <attr name="wheel_unactive_color" format="string"/>
    <attr name="pointer_color" format="string"/>
    <attr name="pointer_halo_color" format="string"/>
    <attr name="text_color" format="string"/>
</declare-styleable>

    <declare-styleable name="CircleTriSeekBar">
        <attr name="triwheel_size" format="integer" />
        <attr name="tripointer_size" format="dimension" />
        <attr name="start_position_1" format="integer"/>
        <attr name="start_position_2" format="integer"/>
        <attr name="start_position_3" format="integer"/>
        <attr name="wheel_color_1" format="string"/>
        <attr name="wheel_color_2" format="string"/>
        <attr name="wheel_color_3" format="string"/>
        <attr name="pointer_color_1" format="string"/>
        <attr name="pointer_color_2" format="string"/>
        <attr name="pointer_color_3" format="string"/>
        <attr name="pointer_halo_color_1" format="string"/>
        <attr name="pointer_halo_color_2" format="string"/>
        <attr name="pointer_halo_color_3" format="string"/>
    </declare-styleable>


</resources>